{
  "name": "taskmanager-system",
  "version": "1.0.0",
  "description": "基于React+Node.js的任务管理系统",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "start:backend": "cd backend && npm start",
    "start:frontend": "cd frontend && npm start",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "build": "cd frontend && npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "task-management",
    "react",
    "nodejs",
    "express",
    "sqlite",
    "antd"
  ],
  "author": "TaskManager Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.7.7",
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SirAlex10086/TaskManager-System.git"
  },
  "bugs": {
    "url": "https://github.com/SirAlex10086/TaskManager-System/issues"
  },
  "homepage": "https://github.com/SirAlex10086/TaskManager-System#readme"
}